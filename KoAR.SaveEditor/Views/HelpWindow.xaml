<Window x:Class="KoAR.SaveEditor.Views.HelpWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" WindowStartupLocation="CenterOwner"
    Title="Help" Height="450" Width="800">
    <DockPanel Margin="5">
        <Button DockPanel.Dock="Bottom" HorizontalAlignment="Center" Content="OK" Command="Close" />
        <TextBlock x:Name="_textBox" TextWrapping="Wrap" Margin="0,0,0,5" />
    </DockPanel>
</Window>
